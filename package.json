{
  "name": "angular2-directives-in-traction",
  "version": "0.0.2",
  "description": "A goto guide for any angular developer to learn angular2 directives/components from ground up",
  "main": "index.html",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "karma start unit.conf.js --singleRun true && npm run test-e2e",
    "test-unit": "karma start unit.conf.js",
    "test-e2e": "./node_modules/protractor/bin/protractor e2e.conf.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" "
  },
  "repository": "",
  "keywords": [
    "angular",
    "directives",
    "grunt",
    "bower",
    "karma",
    "jasmine",
    "protractor"
  ],
  "author": "Amit Gharat a.k.a codef0rmer",
  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-beta.14",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.6",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "bower": "~1.3.12",
    "karma": "~0.12.37",
    "karma-chrome-launcher": "~0.1.3",
    "karma-firefox-launcher": "~0.1.3",
    "karma-jasmine": "~0.1.5",
    "protractor": "^2.1.0",
    "concurrently": "^1.0.0",
    "lite-server": "^1.3.4",
    "typescript": "^1.8.0"
  }
}
