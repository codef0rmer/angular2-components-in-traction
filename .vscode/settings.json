// Place your settings in this file to overwrite the default settings
{
	"editor.tabSize": 2,
	"editor.insertSpaces": true,
  "editor.wrappingColumn": 200

    , // Migrated from previous "File | Auto Save" setting:
    "files.autoSave": "afterDelay",
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/.DS_Store": true,
      "js/**/*.js": true,
      "js/**/*.js.map": true
    }
}